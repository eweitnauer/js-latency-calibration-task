{
  "name": "js-latency-estimation",
  "version": "1.0.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.9.0"
  },
  "scripts": {
    "build-touch": "babel src/touch --out-file lib/js-latency-estimation-touch.js --presets es2015",
    "build-keyboard": "babel src/keyboard --out-file lib/js-latency-estimation-keyboard.js --presets es2015",
    "build": "npm run build-touch && npm run build-keyboard",
    "deploy": "rsync -r -a -v -e 'ssh' keyboard.html touch.html lib css graspablemath.com:/srv/www/graspablemath.com/public_html/projects/js-latency"
  }
}
